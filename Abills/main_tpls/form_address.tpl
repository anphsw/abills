<div class='form-group'>
  <label class='control-label col-md-3' for='COUNTRY'>_{COUNTRY}_:</label>
  <div class='col-md-9'>
    %COUNTRY_SEL%
  </div>
</div>

<div class='form-group'>
  <label class='control-label col-md-3' for='CITY'>_{CITY}_:</label>
  <div class='col-md-4'>
    <input type=text name=CITY value='%CITY%' ID='CITY' class='form-control'>
  </div>

  <label class='control-label col-md-2' for='CONTRACT_DATE'>_{ZIP}_</label>
  <div class='col-md-3'>
   <input type=text name=ZIP value='%ZIP%' class='form-control'>
  </div>
</div>

<div class='form-group'>
  <label class='control-label col-md-3' for='DESCRIBE'>_{ADDRESS_STREET}_:</label>
  <div class='col-md-9'>
    <input type=text name=ADDRESS_STREET value='%ADDRESS_STREET%' ID='ADDRESS_STREET' class='form-control'>
  </div>
</div>

<div class='form-group'>
  <label class='control-label col-md-3' for='DESCRIBE'>_{ADDRESS_BUILD}_:</label>
  <div class='col-md-3'>
    <input type=text name=ADDRESS_BUILD value='%ADDRESS_BUILD%' ID='ADDRESS_BUILD' class='form-control'> 
  </div>

  <label class='control-label col-md-2' for='CONTRACT_DATE'>_{ADDRESS_FLAT}_: </label>
  <div class='col-md-3'>
    <input type=text name=ADDRESS_FLAT value='%ADDRESS_FLAT%' ID='ADDRESS_FLAT' class='form-control'>
  </div>
</div>



