use strict;
use warnings FATAL => 'all';

=head1 NAME

  Sysinfo system information

  http://www.osix.net/modules/article/?id=156

=cut


use Abills::Base;

our (%conf, $db, $admin, %permissions);

if ($permissions{4}){
  require Sysinfo::Reports;
}

if ($permissions{5}){
  require Sysinfo::Configure;
}

1;
