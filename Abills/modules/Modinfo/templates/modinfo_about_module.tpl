<div class='card card-primary card-outline'>
  <div class='card-header'>
    <h4 class='card-title'>
      _{MODULE}_ <b>%MODULE_NAME%</b>
    </h4>
    <div class='card-tools'>
      <a title=_{GUIDE_WIKI_LINK}_ class='btn btn-primary btn-xs my-n1' href='%WIKI_LINK%' target='_blank'>_{GUIDE_WIKI_LINK}_</a>
      <button type='button' title='Show/Hide' class='btn btn-tool' data-card-widget='collapse'>
        <i class='fa fa-minus'></i>
      </button>
    </div>
  </div>

  <div class='card-body' style='overflow: auto; text-align: justify;'>
    %DESCRIBE%
  </div>
</div>