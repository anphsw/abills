<div class='card card-primary card-outline'>
  <div class='card-header with-border'>
    <h4 class='card-title'>%TITLE% (%DATE%)</h4>
    <div class='card-tools float-right'>
      <button type='button' class='btn btn-tool' data-card-widget='collapse'>
        <i class='fa fa-minus'></i>
      </button>
    </div>
  </div>
  <div class='card-body'>
    <div class='row h-100 flex-nowrap'>
      <div class='col d-none d-xl-flex flex-column justify-content-center align-items-center'>
        <img src='%ADMIN_AVATAR%' class='img-circle elevation-2 img-fluid w-75'>
        <div class='text-muted mt-3 mb-0 text-center h6'>%ADMIN%</div>
      </div>
      <div class='col-4 mh-100 text-center'>
        <img src='/styles/default/img/modules/callcenter/arrows.png' class='mh-100'>
      </div>
      <div class='col d-flex flex-column justify-content-center'>
        <h3 class='text-primary text-bold ml-3 h1'>%OUTGOING%</h3>
        <h3 class='text-warning text-bold ml-3 h1'>%TOTAL%</h3>
        <h3 class='text-success text-bold ml-3 h1'>%INCOMING%</h3>
      </div>
    </div>
  </div>
</div>
