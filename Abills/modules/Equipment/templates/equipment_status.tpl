<div class="card box-solid">
  <div class="card-header with-border hidden-print text-center">
    <i class="fa fa-bar-chart-o" style="font-size:18px; margin-right: 5px; float:left;"></i>
    <h4 class="card-title table-caption">%TITLE%</h4>
    <div class="card-tools pull-right">
      <div class="btn-group">
       <button type="button" title="Show/Hide" class="btn btn-tool" data-card-widget="collapse">
       <i class="fa fa-minus"></i>
       </button>
      </div>
    </div>
  </div>
  <!-- /.box-header -->
  <div class="card-body">
    <div class="row">
      %CHARTS%
    </div>
    <!-- /.row -->
  </div>
  <!-- /.box-body -->
</div>

