<div class='col-xs-12 col-md-6'>
  <div class='card card-primary card-outline box-form form-horizontal'>
    <div class='card-body'>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label'>_{DATE}_</label>
        <div class='col-md-9'>
          %DATERANGE_PICKER%
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='IP'>IP</label>
        <div class='col-md-9'>
          <input type=text id='IP' name='IP' value='%IP%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='SID'>SID</label>
        <div class='col-md-9'>
          <input type=text id='SID' name='SID' value='%SID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='HTTP_STATUS'>HTTP STATUS</label>
        <div class='col-md-9'>
          <input type=text id='HTTP_STATUS' name='HTTP_STATUS' value='%HTTP_STATUS%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='HTTP_METHOD'>HTTP METHOD</label>
        <div class='col-md-9'>
          %HTTP_METHOD_SEL%
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='REQUEST_HEADERS'>REQUEST HEADERS</label>
        <div class='col-md-9'>
          <input type=text id='REQUEST_HEADERS' name='REQUEST_HEADERS' value='%REQUEST_HEADERS%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='REQUEST_URL'>REQUEST URL</label>
        <div class='col-md-9'>
          <input type=text id='REQUEST_URL' name='REQUEST_URL' value='%REQUEST_URL%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='REQUEST_BODY'>REQUEST BODY</label>
        <div class='col-md-9'>
          <input type=text id='REQUEST_BODY' name='REQUEST_BODY' value='%REQUEST_BODY%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='RESPONSE'>RESPONSE</label>
        <div class='col-md-9'>
          <input type=text id='RESPONSE' name='RESPONSE' value='%RESPONSE%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='RESPONSE_TIME'>RESPONSE TIME</label>
        <div class='col-md-9'>
          <input type=text id='RESPONSE_TIME' name='RESPONSE_TIME' value='%RESPONSE_TIME%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='ID'>ID</label>
        <div class='col-md-9'>
          <input type=text id='ID' name='ID' value='%ID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='AID'>AID</label>
        <div class='col-md-9'>
          <input type=text id='AID' name='AID' value='%AID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='ERROR_MSG'>ERROR MSG</label>
        <div class='col-md-9'>
          <input type=text id='ERROR_MSG' name='ERROR_MSG' value='%ERROR_MSG%' class='form-control'>
        </div>
      </div>
    </div>
  </div>
</div>
