<div class='col-md-6'>
  <div class='card card-primary card-outline box-big-form'>
    <div class='card-header with-border'>
      <h3 class='card-title'>_{SERVICE}_</h3>
      <div class='card-tools float-right'>
        <button type='button' class='btn btn-tool' data-card-widget='collapse'>
          <i class='fa fa-minus'></i>
        </button>
      </div>
    </div>

    <div class='card-body'>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='TP_ID'>
          Triplay:
        </label>
        <div class='col-md-9'>%TP_ID%</div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='INTERNET_TP'>
          _{INTERNET}_:
        </label>
        <div class='col-md-9'>%INTERNET_TP%</div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='IPTV_TP'>
          _{IPTV}_:
        </label>
        <div class='col-md-9'>%IPTV_TP%</div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='ABON_TP'>
          _{ABON}_:
        </label>
        <div class='col-md-9'>%ABON_TP%</div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='VOIP_TP'>
          _{VOIP}_:
        </label>
        <div class='col-md-9'>%VOIP_TP%</div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='TRIPLAY_EXPIRE'>
          _{EXPIRE}_ _{DATE}_:</label>
        <div class='col-md-9'>
          <input id='TRIPLAY_EXPIRE' name='TRIPLAY_EXPIRE' value='%TRIPLAY_EXPIRE%'
                 class='form-control datepicker' type='text'/>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 control-label' for='SERVICE_STATUS'>
          _{STATUS}_:
        </label>
        <div class='col-md-9'>%SERVICE_STATUS%</div>
      </div>

    </div>
  </div>
</div>


