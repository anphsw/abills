#!/usr/bin/perl -w

=head1 NAME

   billd

Accounting control deamon
Speed control
Work only with Dv module

=cut

use strict;

BEGIN {
  our $libpath = '../';
  my $sql_type = 'mysql';
  use FindBin '$Bin';
  unshift( @INC,
    $Bin . '/' . $libpath . "Abills/$sql_type/",
    $Bin . '/' . $libpath . "Abills/modules/",
    $Bin . '/' . $libpath . '/lib/' );

  eval { require Time::HiRes; };
  our $begin_time = 0;
  if ( !$@ ){
    Time::HiRes->import( qw(gettimeofday) );
    $begin_time = Time::HiRes::gettimeofday();
  }
}

our (%conf,
  @MODULES,
  $DATE,
  $TIME,
  $IPFW,
  $IFCONFIG,
  $var_dir,
  $base_dir,
);

use Abills::Defs;
use Abills::Nas::Control;
use Abills::Base qw(check_time gen_time parse_arguments int2ip ip2int time2sec mk_unique_value in_array cmd ssh_cmd _bp startup_files);
use Abills::Server;
use Users;
use Admins;
use Tariffs;
use Dv_Sessions;
use Dv;
use Nas;
use Billing;
use Log qw(log_add);
use Conf;

require $Bin . '/config.pl';

my $begin_time = check_time();

our $debug = 0;
our $argv = parse_arguments( \@ARGV );
my $debug_output = '';
my $log_dir = $var_dir . '/log';
my $plugins = '';

if ( defined( $argv->{DEBUG} ) ){
  $debug = $argv->{DEBUG} || 1;
  print "Debug mode $debug\n";
}
elsif ( defined( $argv->{help} ) ){
  #get plugins
  opendir my $dh, "$Bin/billd.plugins/" or die "Can't open dir '$Bin/billd.plugins/' $!\n";
  my @contents = grep /\.pm$/, readdir $dh;
  closedir $dh;

  foreach my $p ( @contents ){
    $p =~ s/\.pm//;
    $plugins .= "  $p\n";
  }

  help();
  exit;
}

exit if (!$argv->{checkppp} && make_pid( $log_dir . "/billd.pid" ) == 1);

our $db = Abills::SQL->connect( $conf{dbtype}, $conf{dbhost}, $conf{dbname}, $conf{dbuser}, $conf{dbpasswd}, \%conf );
if ( !$db->{db} ){
  print "Error: SQL connect error\n";
  exit;
}

our $Admin = Admins->new( $db, \%conf );

$Admin->info( $conf{SYSTEM_ADMIN_ID}, { IP => '127.0.0.1' } );
if ( $Admin->{errno} ){
  print "AID: $conf{SYSTEM_ADMIN_ID} [$Admin->{errno}] $Admin->{errstr}\n";
  exit 0;
}

my $exppp_hanguplimit = 3500000000;
my $error_alive_count = $conf{ERROR_ALIVE_COUNT} || 3;

our $OS = $^O;
if ( $argv->{OS} ){
  $OS = $argv->{OS};
}

$conf{MINIMUM_SESSION_TIME} = 0;
$conf{MINIMUM_SESSION_TRAF} = 0;

our %LIST_PARAMS = ();
#my $Conf = Conf->new( $db, $Admin, \%conf );
my $Tariffs = Tariffs->new( $db, \%conf, $Admin );
our $Sessions = Dv_Sessions->new( $db, $Admin, \%conf );
our $Dv = Dv->new( $db, $Admin, \%conf );
our $Nas = Nas->new( $db, \%conf );
our $Log = Log->new( $db, \%conf );
$Log->{PRINT} = 1;

my $Nas_cmd = Abills::Nas::Control->new( $db, \%conf );

if ( $argv->{NAS_IDS} ){
  $LIST_PARAMS{NAS_IDS} = $argv->{NAS_IDS};
  $LIST_PARAMS{NAS_IDS} =~ s/ //g;
  $LIST_PARAMS{NAS_IDS} =~ s/,/;/g;
  $LIST_PARAMS{NAS_ID} = $LIST_PARAMS{NAS_IDS};
}

$LIST_PARAMS{TYPE} = $argv->{NAS_TYPES} if ($argv->{NAS_TYPES});
$LIST_PARAMS{USER_NAME} = $argv->{LOGINS} if ($argv->{LOGINS});
$LIST_PARAMS{TP_ID} = $argv->{TP_IDS} if ($argv->{TP_IDS});
$LIST_PARAMS{GID} = $argv->{GIDS} if ($argv->{GIDS});
$LIST_PARAMS{PAGE_ROWS} = 100000;
$LIST_PARAMS{DISABLE} = 0;

$conf{KBYTE_SIZE} = 1024 if (!$conf{KBYTE_SIZE});
$conf{MB_SIZE} = $conf{KBYTE_SIZE} * $conf{KBYTE_SIZE};

if ( defined( $argv->{checkppp} ) ){ check_ifaces(); }
elsif ( defined( $argv->{checkpppd} ) ){ check_ifaces( { LINUX => 1 } ); }
elsif ( defined( $argv->{checkmpd} ) ){ check_ifaces( { MPD => 1 } ); }
elsif ( defined( $argv->{checkmpd2} ) ){ check_mpd();   }
elsif ( defined( $argv->{checkspeed} ) ){ check_speed(); }
elsif ( defined( $argv->{check_pptpd} ) ){ check_pptpd(); }
elsif ( defined( $argv->{checkmikro} ) ){ check_mikro(); }
elsif ( defined( $argv->{speed_expr} ) ){ speed_expr();  }
elsif ( defined( $argv->{check_cisco_cid} ) ){ check_cisco_cid() }
elsif ( defined( $argv->{check_cisco_if} ) ){ check_cisco_if() }
else{
  my $action = $ARGV[0];
  if ( $action && -f "$Bin/billd.plugins/$action" . '.pm' ){
    billd_plugins( $action );
  }
  else{
    check_lines();
    run_plugins();
  }
}

if ( $debug > 0 ){
  print gen_time( $begin_time );
}

make_pid( $log_dir . "/billd.pid", 'clean' );

