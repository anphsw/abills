{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "color": {
      "type": [
        "string",
        "integer"
      ],
      "pattern": "^[0-9A-Fa-f]{6}$"
    },
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "integer"
    },
    "getFees": {
      "type": "integer"
    }
  },
  "required": [
    "color",
    "id",
    "name",
    "type",
    "getFees"
  ]
}
